{
  "description": "A CLI tool implementing the beehive flow git branching process",
  "keywords": [
    "git"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "bugs": "https://github.com/tinymce/beehive-flow/issues",
  "author": "Tiny Technologies",
  "contributors": [],
  "homepage": "https://github.com/tinymce/beehive-flow",
  "main": "lib/main/ts/Main.js",
  "repository": "https://github.com/tinymce/beehive-flow",
  "license": "Apache-2.0",
  "dependencies": {
    "cross-spawn-promise": "^0.10.2",
    "fp-ts": "^2.9.5",
    "io-ts": "^2.2.14",
    "luxon": "^1.26.0",
    "properties-reader": "^2.2.0",
    "read-pkg": "^5.2.0",
    "simple-git": "^2.35.1",
    "tmp": "^0.2.1",
    "tslib": "^2.1.0",
    "yargs": "^16.2.0"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "test": "yarn nyc mocha",
    "beehive-flow": "node -r 'ts-node/register' src/main/ts/Main.ts",
    "buildLintTest": "yarn build && yarn lint && yarn test",
    "buildAndInstall": "yarn build && yarn global add file:$PWD"
  },
  "devDependencies": {
    "@tinymce/eslint-plugin": "^1.7.2",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/kind-of": "^6.0.0",
    "@types/luxon": "^1.26.0",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@types/properties-reader": "^2.1.0",
    "@types/tmp": "^0.2.0",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@verdaccio/types": "^9.7.2",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.20.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "esm": "^3.2.25",
    "fast-check": "^2.13.0",
    "get-port": "^5.1.1",
    "mocha": "^8.3.0",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-multi-reporters": "^1.5.1",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5",
    "verdaccio": "^4.11.0"
  },
  "bin": {
    "beehive-flow": "lib/main/ts/Main.js"
  },
  "files": [
    "lib/main",
    "src"
  ],
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "version": "0.17.0-rc",
  "name": "@tinymce/beehive-flow"
}
